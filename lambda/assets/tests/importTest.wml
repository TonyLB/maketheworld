<Asset key=(ImportTest) fileName="importTest" zone="Personal" subFolder="TonyLB/" player="TonyLB">
    <Variable key=(active) default={true} />
    <Action key=(toggleActive) src={active = !active} />
    <Import from=(TEST)>
        <Use key=(345) as=(welcomeRoom) />
    </Import>
    <Room key=(welcomeRoom)>
        <Link key=(switch) to=(toggleActive)>(Flip switch)</Link>
    </Room>
    <Condition if={active}>
        <Depend on=(active) />
        <Room key=(welcomeRoom)>
            It's a welcome area, with bright lighting and pastel walls.
        </Room>
    </Condition>
</Asset>